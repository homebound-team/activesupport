    "jest-junit": "^16.0.0",
    "lint-staged": ">=10",
    "pinst": ">=2",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "semantic-release": "^24.2.5",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "5.8.3"
  },
  "lint-staged": {
    "*.{js,ts,css,md}": "prettier --write",
    "*.{js,ts,tsx}": "eslint --fix"
